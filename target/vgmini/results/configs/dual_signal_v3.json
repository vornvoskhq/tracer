{
  "name": "dual_signal_v3",
  "description": "Enhanced dual signal strategy - optimized based on feature importance analysis, removes weak features and adds momentum focus",
  "symbols": [
    "SPY",
    "QQQ"
  ],
  "start_date": "2023-01-01",
  "end_date": "2024-01-01",
  "buy_threshold": 0.025,
  "sell_threshold": 0.015,
  "forecast_horizon": 10,
  "confidence_threshold": 0.70,
  "ema_periods": [
    9,
    21,
    50
  ],
  "volume_window": 15,
  "test_size": 0.2,
  "class_weight": "balanced",
  "penalty": "l1",
  "solver": "liblinear",
  "initial_capital": 100000.0,
  "commission": 0.001,
  "max_position_size": 0.15,
  
  "technical_indicators": {
    "rsi": {
      "period": 14,
      "oversold_threshold": 30,
      "overbought_threshold": 70
    },
    "macd": {
      "fast_period": 8,
      "slow_period": 21,
      "signal_period": 5
    },
    "bollinger_bands": {
      "period": 20,
      "std_dev": 2
    },
    "momentum": {
      "window": 3,
      "std_window": 15,
      "std_multiplier": 1.2
    },
    "roc_periods": [3, 5, 10],
    "volume_roc_periods": [3, 5]
  },
  
  "enabled_features": [
    "ema9_above_ema50",
    "price_roc_10",
    "price_above_ema30",
    "macd_positive_hist",
    "macd_above_signal",
    "ema30_above_ema50",
    "price_roc_5",
    "price_roc_3",
    "ema_spread_accelerating",
    "price_momentum_strong",
    "volume_roc_5",
    "volume_roc_3",
    "macd_above_zero",
    "signal_above_zero",
    "price_ema_separation_accel",
    "volume_above_average"
  ],
  
  "analysis": {
    "interpretability_sample_size": 1000,
    "lime_sample_size": 100,
    "lime_max_samples": 100,
    "min_valid_target_ratio": 0.8,
    "max_correlation_threshold": 0.95,
    "rolling_sharpe_window": 60,
    "trading_days_per_year": 252
  },
  
  "visualization": {
    "dpi": 300,
    "figure_size": [12, 8]
  }
}